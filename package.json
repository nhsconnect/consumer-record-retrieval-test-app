{
    "name": "nrl-consumer-retrieval-test-app",
    "version": "0.0.1",
    "description": "NHS Digital NRL Consumer Retrieval Test App",
    "main": "main.js",
    "scripts": {
        "cpf": "copyfiles -u 1 \"src/static/**\" \"src/config.json\" dist",
        "build-dev": "rm -rf ./dist && mkdir dist && node make-doc.js && npm run cpf && tsc",
