        "dev": "npm run build-dev && npm run start-dev",
        "tsc": "tsc"
    },
    "directories": {
        "lib": "lib",
        "reports": "reports",
        "node_modules": "node_modules"
    },
    "author": "lionel.rowe1@nhs.net",
    "license": "OGL",
    "devDependencies": {
        "@types/express": "^4.17.1",
        "@types/morgan": "^1.7.37",
        "chalk": "^2.4.2",
        "copyfiles": "^2.1.1",
        "express": "^4.17.1",
        "morgan": "^1.9.1",
        "newman": "^4.5.5",
        "newman-reporter-html": "^1.0.4",
        "showdown": "^1.9.0",
        "typescript": "^3.6.4"
    },
    "dependencies": {}
}
